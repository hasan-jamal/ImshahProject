                    <div class="col-xl-2 col-md-4 mb-30 col-6">
                        <div class="single-cta">
                            <i class="fas fa-map-marker-alt"></i>
                            <div class="cta-text">
                                <h4>Find us</h4>
                                <span>@information.FindUs</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-2 col-md-4 mb-30 col-6">
                        <div class="single-cta">
                            <i class="fas fa-phone"></i>
                            <div class="cta-text">
                                <h4>Call us</h4>
                                <span>@information.Phone</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-2 col-md-4 mb-30 col-6">
                        <div class="single-cta">
                            <i class="far fa-envelope-open"></i>
                            <div class="cta-text">
                                <h4>Mail us</h4>
                                <span>@information.Email</span>
