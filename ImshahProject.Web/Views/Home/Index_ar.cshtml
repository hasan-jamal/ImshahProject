@model TablesVM

@{
    ViewData["Title"] = "Home Page";
}
<a href="#contact" class="js-anchor-link btn-con">تواصل معنا </a>
<!-- Header -->
<div class="navik-header header-shadow style-arabic">

    <div class="style-language">
        <div class="container d-flex align-items-center justify-content-end">
            <div class="d-flex align-items-center mx-1">
                <img src="~/images/english.png" alt="english" width="20" class="mx-1">
                <a asp-controller="Home" asp-action="Index">EN</a>
            </div>
            <a href="#" class="mx-1">|</a>
            <div class="d-flex align-items-center mx-1">
                <img src="~/images/arabic.png" alt="arabic" width="20" class="mx-1">
                <a asp-controller="Home" asp-action="Index_ar">AR</a>
            </div>
        </div>

    </div>
    <div class="container style-arabic">
        <!-- Navik header -->
        <div class="navik-header-container">

            <!--Logo-->
            <div class="logo" data-mobile-logo="/images/logoNew.png"
                 data-sticky-logo="/images/logoNew.png">
                <a href="#home"><img src="/images/logoNew.png" alt="logo" /></a>
            </div>

            <!-- Burger menu -->
            <div class="burger-menu">
                <div class="line-menu line-half first-line"></div>
                <div class="line-menu"></div>
                <div class="line-menu line-half last-line"></div>
            </div>

            <!--Navigation menu-->
            <nav class="navik-menu menu-caret submenu-top-border submenu-scale">
                <ul class="list-unstyled">
                    <li>
                        <a href="#home" class="js-anchor-link"><i class="fas fa-home mx-1"></i>الرئيسية</a>
                    </li>
                    <li><a href="#about" class="js-anchor-link">من نحن</a></li>
                    <li><a href="#services" class="js-anchor-link">خدماتنا</a></li>
                    <li><a href="#partners" class="js-anchor-link">شركائنا</a></li>
                    <li><a href="#contact" class="js-anchor-link">تواصل معنا</a></li>
                    <partial name="~/Views/Shared/_btn_dash_ar.cshtml" />
                      <li>
                        @foreach (var information in Model.Information)
                        {
                            <a class="button--gradient js-modal dis-none" data-modal="#modal_2">@information.GetAquote_ar</a>
                        }
                    </li>
                </ul>
                @foreach (var information in Model.Information)
                {
                    <a class="button--gradient js-modal dis-block" data-modal="#modal_2">@information.GetAquote_ar</a>
                }
            </nav>

        </div>

    </div>
</div>
<!---Advance Slider--->
<div class="container-slide style-arabic">
            <div id="overflow-all"></div>
            <div class="arrow arrow-left"><i class="fas fa-chevron-circle-left"></i></div>
            @foreach (var slider in Model.Sliders)
             {
            <div class="img">
                <img src="~/Components/Images/Sliders/@slider.ImageUrl" alt="">
                <div class="box-text">
                    <h1>@slider.Name_ar</h1>
                    <p>@slider.Text_ar</p>
                </div>
            </div>
             }
         
            <div class="arrow arrow-right"><i class="fas fa-chevron-circle-right"></i>
            </div>
         <div class="dots">
              <div class="dot">
                <i class="far fa-dot-circle"></i>
              </div>
              <div class="dot">
                <i class="far fa-circle"></i>
              </div>
              <div class="dot">
                <i class="far fa-circle"></i>
              </div>
            </div>
          </div>
<main class="style-arabic">
    <section class="sectionAbout aboutPadding  block">
        <div class="container" id="about">
            <div class="row align-items-center ">
                @foreach (var about in Model.About)
                {
                    <div class="col-md-6">
                        <div class="sectionAbout-text">
                            <h3 data-animation="slideInRight">من نحن ؟</h3>
                            <p >
                                @about.Text_ar1
                            </p>

                            <h3 data-animation="slideInRight">مهمتنا</h3>
                            <p >
                                @about.Text_ar2
                            </p>

                            <h3 data-animation="slideInRight">رؤيتنا</h3>
                            <p >
                                @about.Text_ar3
                            </p>
                            <button onclick="window.location.href='#contact';"
                                class="btn-text mt-4">
                                تواصل معنا
                            </button>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="sectionAbout-images">
                            <img src="~/Components/Images/Abouts/@about.ImageUrl" alt="about" class="img1" >
                            <img src="~/Components/Images/Abouts/@about.ImageUrl2" alt="about" class="img2">
                        </div>
                    </div>

                }

            </div>
        </div>
    </section>
    <section class="our-servcies block" id="services">
        <div class="container">
            <h3 class="heading-tertiary mt-5">خدماتنا</h3>
            <p class="sub-heading sub-heading--black">نقدم العديد من الخدمات البرمجية والتقنية , نمتلك فريق تقني مميز قادر على إنجاز المشاريع وتحويل فكرتك إلى مشروع برمجي .</p>
            <div class="row">
                @foreach (var service in Model.Services)
                {
                    <div class="col-md-4">
                        <div class="box-service">
                            <div class="box-service-img">
                                <img src="~/Components/Images/Services/@service.ImageUrl" alt="coding">
                            </div>
                            <h4 class="text-center">@service.Name_ar</h4>
                        </div>
                    </div>
                }

            </div>
        </div>
    </section>
    <div class="sectionAbout ">
        <div class="container">
            <div class="row ">
                @foreach (var goal in Model.Goals)
                {
                    <div class="col-md-6">
                        <div class="sectionAbout-images sectionAbout2-img">
                            <img src="~/Components/Images/Goals/@goal.ImageUrl" alt="about" class="img1">
                             <img src="~/Components/Images/Goals/@goal.ImageUrl2" alt="about" class="img1">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="sectionAbout-text">
                            <h3>@goal.Name_ar</h3>
                            <p id="text">@goal.Text_ar </p>
                            <button class="btn-text" id="btnAr" onclick="toggleTextAr(this)">قراءة المزيد</button>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    <section class="section-contact block block-pad" id="contact">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="section-contact-text">
                        <h1>تواصل معنا</h1>
                        <h5 ata-animation="slideInUp">
                            تطبيقات وخدمات IMSHAH AI
                        </h5>
                        @foreach (var information in Model.Information)
                        {
                            <p class="mt-5" ata-animation="slideInUp" >@information.FindUs_ar</p>
                            <p ata-animation="slideInUp">@information.Workinghours</p>
                            <p ata-animation="slideInUp">@information.Email</p>
                            <p ata-animation="slideInUp">@information.Phone</p>
                        }
                    </div>
                </div>
                <div class="col-md-6">
                    <form asp-controller="Contacts" asp-action="Create" method="post" class="form-style" data-animation="flipInY" data-animation-delay="600ms">
                        <div class="row align-items-center ">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="firstName">الاسم الأول :</label>
                                    <input type="text" id="firstName" name="firstName" class="form-control"
                                           placeholder="اضف الإسم الأول">
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="lastName">الاسم الثاني :</label>
                                    <input type="text" id="lastName" name="lastName" class="form-control"
                                           placeholder="اضف الإسم الأخير">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="email">الإيميل :</label>
                            <input type="email" id="email" name="email" class="form-control"
                                   placeholder="اضف الإيميل">
                        </div>
                         <div class="form-group">
                            <label for="subject">عنوان الرسالة :</label>
                            <input type="text" id="subject" name="subject" class="form-control"
                                   placeholder="اضف عنوان الرسالة">
                        </div>
                        <div class="form-group">
                            <label for="message">الرسالة :</label>
                            <textarea type="text" id="message" name="message" rows="4" class="form-control"
                                      placeholder="اضف رسالتك"></textarea>
                        </div>
                        <div class="form-group">
                            <button type="submit">إرسال</button>
                        </div>
                    </form>

                </div>
            </div>
        </div>
        <img src="~/images/back.png" alt="" class="img-back">
    </section>
    <section class="section-Specialists">
        <div class="section-Specialists-text">
            <div class="container-padding">
                <h4>منتجاتنا </h4>
                <div class="row align-items-center justify-content-center">
                    <div class="col-md-4 col-6 my-2">
                        <button data-animation="slideInUp" data-animation-delay=".3s"> All kinds of Portal</button>
