    <section class="section2-slider block visitors" id="partners">
        <h4>زوار الموقع</h4>
        <div class="container">
            <h1 id="count" class="count-style">0</h1>
        </div>
    </section>

    <section class="section2-slider block" id="partners">
        <h4 data-animation="flipInY">شركائنا وعملائنا </h4>
        <div class="container">
            <div class="wrapper">
                <div class="carousel">
                    @foreach (var partner in Model.Partner)
                    {
                        <div class="card-div product">
                            <img src="~/Components/Images/Partners/@partner.ImageUrl" alt="">
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>
</main>

